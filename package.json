    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "packageManager": "yarn@4.6.0",
    "keywords": [
        "react",
        "moon",
        "widget",
        "astronomy"
