    "license": "MIT",
    "peerDependencies": {
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/miksrv/moon-widget.git"
    },
    "dependencies": {
        "pixi.js": "^8.8.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-scripts": "5.0.1",
        "suncalc": "^1.9.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.7",
        "@eslint/js": "^9.22.0",
        "@types/prop-types": "^15.7.14",
        "@types/react": "^19.0.11",
        "@types/react-dom": "^19.0.4",
        "eslint": "^9.22.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-eslint-plugin": "^6.4.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.11.0",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "prettier": "^3.5.3",
        "sass": "^1.86.0",
        "sass-loader": "^16.0.5",
        "tsup": "^8.4.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.1"
